            "cypressConfig": "apps/ecommerce-e2e/cypress.json",
            "devServerTarget": "ecommerce:serve:development",
            "tsConfig": "apps/ecommerce-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
